{
  "version": 2,
  "functions": {
    "api/post_info.py": {
      "maxDuration": 10
    },
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/post_info",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/post_info.py"
    },
    {
      "src": "/api/top_phrases",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}